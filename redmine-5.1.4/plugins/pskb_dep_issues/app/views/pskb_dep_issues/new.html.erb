<h2>
  <%= link_to 'Подразделения', pskb_dep_issues_path %>
  >> Создать
</h2>
<%= form_with(model: @dep_issue, url: pskb_dep_issues_path) do |form| %>

  <div>
    <%= form.label :user_id, 'Voter' %>
    <%= form.collection_select :user_id, @users, :id, :name, { prompt: 'Select Voter' } %>
    <%= form.label :dep_id, 'Department' %>
    <%= form.collection_select :dep_id, @departments, :id, :name, { prompt: 'Select Department' } %>
  </div>

  <div style="margin: 10px 0px;">
    <%= form.submit 'Создать' %>
  </div>
<% end %>