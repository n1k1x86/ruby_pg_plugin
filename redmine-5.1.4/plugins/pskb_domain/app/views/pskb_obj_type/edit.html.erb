<h2>
  <%= link_to 'Типы объектов PSKB', pskb_obj_type_index_path %>
  >> Редактировать
</h2>

<%= form_with model: @pskb_obj_type, method: :put, url: pskb_obj_type_path(@pskb_obj_type), local: true do |form| %>

  <div>
    <%= form.label :name, 'Name' %>
    <%= form.text_field :name %>

    <%= form.label :description, 'Description' %>
    <%= form.text_field :description %>
  </div>

  <div style="margin: 10px 0px;">
    <%= form.submit 'Изменить' %>
  </div>
<% end %>
